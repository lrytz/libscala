#!/usr/bin/env bash
#
# class name -> class file

source /r/jbash/jbash

jbash_usage="[class class ...]"
jbash_help=$(cat <<EOM
For each given class, prints the path to its classfile, which
will be extracted from the jar if necessary.
EOM
)
jbash-command "$0" "$@"

for arg in "${jbash_args[@]}"; do
  cp-find-class "$arg"
done

